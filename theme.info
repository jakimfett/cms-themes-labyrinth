name           = "labyrinth"
title          = "Labyrinth Theme"
description    = "Semi-responsive theme for the Labyrinth website"
version        = 0.0.1
license        = 
themeURL       = https://github.com/jakimfett/cms-themes-labyrinth
depends        = pages
author         = jakimfett
authorURL      = http://jakimfett.com
site           = 1
admin          = 1

;
; Regions
;
; format:
; [regions]
; 	REGION_NAME = REGION TITLE
;
; Don't write here titles regions using national character encoding
; or non-english language. Region titles must be localized into your
; language using __() method
;
[regions]
	content_top    = The top of the main content
	content_bottom = The bottom of the main content
	top            = Top Region
	bottom         = Bottom Region
	left           = Left Sidebar
	right          = Right Sidebar
	dashboard      = Dashboard
	footer         = Footer
